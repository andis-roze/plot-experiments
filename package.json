{
  "name": "plot-experiments",
  "version": "0.0.1",
  "description": "Plot experiments",
  "main": "index.tsx",
  "author": {
    "name": "Andis Roze",
    "url": "http://"
  },
  "private": true,
  "scripts": {
    "dist:dev": "cross-env TS_NODE_PROJECT=\"tsconfig.webpack.json\" webpack --envNODE_ENV=dev --config ./webpack.config.ts",
    "dist:test": "cross-env TS_NODE_PROJECT=\"tsconfig.webpack.json\" webpack --env NODE_ENV=test --config ./webpack.config.ts",
    "dist:prod": "cross-env TS_NODE_PROJECT=\"tsconfig.webpack.json\" webpack --env NODE_ENV=prod --config ./webpack.config.ts",
    "tslint": "tslint 'src/**/*.ts' --format stylish",
    "watch": "cross-env TS_NODE_PROJECT=\"tsconfig.webpack.json\" webpack --watch",
    "start": "cross-env TS_NODE_PROJECT=\"tsconfig.webpack.json\" webpack serve --env NODE_ENV=dev --watch --progress --host 0.0.0.0",
    "start:prod": "cross-env TS_NODE_PROJECT=\"tsconfig.webpack.json\" webpack serve --env NODE_ENV=prod --watch --progress --host 0.0.0.0",
    "stats": "yarn dist:prod; webpack-bundle-analyzer dist/stats.json ./dist/production --no-open;",
    "test": "JEST_JUNIT_OUTPUT=./test-results/junit.xml jest --config ./jest.config.json",
    "test:watch": "yarn test --watch ",
    "coverage": "yarn test --coverage"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.11",
    "@types/enzyme": "^3.10.8",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.16",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/webpack": "^4.41.25",
    "@types/webpack-dev-server": "^3.11.1",
    "autoprefixer": "^10.1.0",
    "circular-dependency-plugin": "^5.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "html-webpack-plugin": "^5.0.0-beta.1",
    "jest": "^26.6.3",
    "jest-junit": "^12.0.0",
    "postcss": "^8.2.2",
    "postcss-loader": "^4.1.0",
    "sass": "^1.32.0",
    "sass-loader": "^10.1.0",
    "style-loader": "^2.0.0",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.12",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.3",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-loader": "^3.5.4",
    "typedoc": "^0.20.4",
    "typescript": "^4.1.3",
    "webpack": "^5.11.1",
    "webpack-bundle-analyzer": "^4.3.0",
    "webpack-cli": "^4.3.0",
    "webpack-dev-server": "^3.11.1"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "function-plot": "^1.22.4",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hook-form": "^6.14.0"
  }
}
